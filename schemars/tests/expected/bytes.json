{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Tuple_of_Bytes_and_Bytes",
  "type": "array",
  "prefixItems": [
    {
      "$ref": "#/$defs/Bytes"
    },
    {
      "$ref": "#/$defs/Bytes"
    }
  ],
  "minItems": 2,
  "maxItems": 2,
  "$defs": {
    "Bytes": {
      "type": [
        "array",
        "string"
      ],
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0
      }
    }
  }
}
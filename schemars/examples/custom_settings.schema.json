{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MyStruct",
  "type": "object",
  "required": [
    "my_bool",
    "my_int",
    "my_nullable_enum"
  ],
  "properties": {
    "my_bool": {
      "type": "boolean"
    },
    "my_int": {
      "type": "integer",
      "format": "int32"
    },
    "my_nullable_enum": {
      "allOf": [
        {
          "$ref": "#/definitions/MyEnum"
        }
      ],
      "nullable": true
    }
  },
  "definitions": {
    "MyEnum": {
      "anyOf": [
        {
          "properties": {
            "StringNewType": {
              "type": "string"
            }
          },
          "required": [
            "StringNewType"
          ],
          "type": "object"
        },
        {
          "properties": {
            "StructVariant": {
              "properties": {
                "floats": {
                  "items": {
                    "format": "float",
                    "type": "number"
                  },
                  "type": "array"
                }
              },
              "required": [
                "floats"
              ],
              "type": "object"
            }
          },
          "required": [
            "StructVariant"
          ],
          "type": "object"
        }
      ]
    }
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Process",
  "type": "object",
  "required": [
    "command_line",
    "durations",
    "wall_time"
  ],
  "properties": {
    "command_line": {
      "type": "string"
    },
    "durations": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/Duration"
      }
    },
    "wall_time": {
      "$ref": "#/$defs/Duration"
    }
  },
  "$defs": {
    "Duration": {
      "type": "object",
      "required": [
        "nanos",
        "secs"
      ],
      "properties": {
        "nanos": {
          "type": "integer",
          "format": "int32"
        },
        "secs": {
          "type": "integer",
          "format": "int64"
        }
      }
    }
  }
}
